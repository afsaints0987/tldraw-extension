{
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx":"react",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "module": "commonjs",
    "lib": ["ES6", "DOM", "DOM.Iterable"],
    "types": [
      "@extpkg/types-console",
      "@extpkg/types-runtime",
      "@extpkg/types-tabs",
      "@extpkg/types-webviews",
      "@extpkg/types-windows",
      "@extpkg/types-websessions",
      "@types/react",
      "@types/react-dom"
    ]
  },
  "include": ["src/main.ts"]
}
