{
  "manifest_version": "ext-v1",
  "name": "TLDraw - Extension",
  "short_name": "TLDraw",
  "version": "1.0.0",
  "description": "an extension for tldraw a collaborative digital whiteboard",
  "author": "Aaron Santos",
  "homepage_url": "https://github.com/tldraw/tldraw",
  "icons": {
    "128": "assets/icon-1024.png",
    "background": "assets/icon-background.png",
    "banner": "assets/icon-banner.png",
    "128-dark": "assets/icon-128-dark.png",
    "background-dark": "assets/icon-background-dark.png",
    "banner-dark": "assets/icon-banner-dark.png"
  },
  "scripts": ["main.js"],
  "modules": {
    "console": {
      "module_version": "1.0.1",
      "permissions": ["*"]
    },
    "runtime": {
      "module_version": "1.2.1",
      "permissions": ["*"]
    },
    "tabs": {
      "module_version": "1.1.0",
      "permissions": ["*"]
    },
    "windows": {
      "module_version": "1.1.0",
      "permissions": ["*"]
    },
    "webviews": {
      "module_version": "1.0.2",
      "permissions": ["*"]
    },
    "websessions": {
      "module_version": "1.0.1",
      "permissions": ["*"]
    }
  },
  "permissions": ["*"]
}
